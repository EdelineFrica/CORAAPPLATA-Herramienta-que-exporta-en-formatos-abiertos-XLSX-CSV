@model IEnumerable<AppDatosAbiertos.Mes>

@{
    ViewBag.Title = "Index";
}

<center>
    <h2>
        Registro Meses
    </h2>
</center>
<p>
    <a class="btn btn-primary" style="margin-bottom:10px" href="/Mes/Create">
        <span class='glyphicon glyphicon-file'></span>
    </a>
</p>
<table class="table-bordered">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NombreMes)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NombreMes)
        </td>
        <td>
            @Html.Raw("<a data-modal='' href='/Mes/Edit/" + item.IdMes + "' id='" + item.IdMes + "'title='Edit'> <span class='glyphicon glyphicon-edit'></span> </a>")
            @Html.Raw("<a data-modal='' href='/Mes/Details/" + item.IdMes + "' id='" + item.IdMes + "'title='Details'> <span class='glyphicon glyphicon-th-list'></span> </a>")
            @Html.Raw("<a data-modal='' href='/Mes/Delete/" + item.IdMes + "' id='" + item.IdMes + "'title='Delete'> <span class='glyphicon glyphicon-trash'></span> </a>")
        </td>
    </tr>
    }

</table>
